#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int n,sum=0;             //use long if difit is greater then 10 digit.
    scanf("%d", &n);
    while(n!=0)             //while loop until the end of the number.
    {
        sum=sum+n%10;
        n=n/10;
    }
    printf("%d",sum);
    //Complete the code to calculate the sum of the five digits on n.
    return 0;
}
